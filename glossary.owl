        rdf:type        owl:Class ;
        rdfs:label      "Work Package 2.1"@en-US ;
        ncit:C42610     "WP2.1"@en-US ;
        dc:description  "In vitro models to provide toxicokinetics & toxicodynamics parameters: objectives to verify in vitro models for ADME and local toxicity, obtaining parameters for PBK modelling, test chemicals on iPSC models."@en-US .

